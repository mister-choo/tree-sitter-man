{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "man",
  "rules": {
    "manual": {
      "type": "SEQ",
      "members": [
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "\\S[^\\r\\n]*\\r?\\n"
          },
          "named": true,
          "value": "title"
        },
        {
          "type": "PATTERN",
          "value": "\\r?\\n"
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "SYMBOL",
            "name": "section"
          }
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "\\S[^\\r\\n]*\\r?\\n"
          },
          "named": true,
          "value": "footer"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "PATTERN",
              "value": "\\r?\\n"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "section": {
      "type": "SEQ",
      "members": [
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "\\S[^\\r\\n]*\\r?\\n"
          },
          "named": true,
          "value": "section_title"
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "body"
              },
              {
                "type": "SYMBOL",
                "name": "subsection_heading"
              }
            ]
          }
        }
      ]
    },
    "subsection_heading": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "PATTERN",
            "value": " {3}"
          },
          {
            "type": "ALIAS",
            "content": {
              "type": "PATTERN",
              "value": "\\S[^\\r\\n]*\\r?\\n"
            },
            "named": true,
            "value": "subsection_title"
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "SYMBOL",
              "name": "body"
            }
          }
        ]
      }
    },
    "body": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "REPEAT1",
        "content": {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "option_section"
            },
            {
              "type": "SYMBOL",
              "name": "_block"
            }
          ]
        }
      }
    },
    "_block": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "REPEAT1",
            "content": {
              "type": "SYMBOL",
              "name": "_line"
            }
          },
          {
            "type": "PATTERN",
            "value": "\\r?\\n"
          }
        ]
      }
    },
    "_text": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "reference"
            },
            {
              "type": "TOKEN",
              "content": {
                "type": "PREC",
                "value": -2,
                "content": {
                  "type": "PATTERN",
                  "value": "[^\\r\\n]\\S*"
                }
              }
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "TOKEN",
                "content": {
                  "type": "PREC",
                  "value": -1,
                  "content": {
                    "type": "PATTERN",
                    "value": "\\S+"
                  }
                }
              },
              {
                "type": "SYMBOL",
                "name": "reference"
              },
              {
                "type": "PATTERN",
                "value": " +"
              }
            ]
          }
        }
      ]
    },
    "_line_10": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": " {10}"
        },
        {
          "type": "SYMBOL",
          "name": "_text"
        },
        {
          "type": "PATTERN",
          "value": "\\r?\\n"
        }
      ]
    },
    "_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": " {7}"
        },
        {
          "type": "SYMBOL",
          "name": "_text"
        },
        {
          "type": "PATTERN",
          "value": "\\r?\\n"
        }
      ]
    },
    "reference": {
      "type": "PATTERN",
      "value": "[a-zA-Z0-9_][a-zA-Z0-9_\\-]+\\(\\d+\\)"
    },
    "short_option": {
      "type": "PATTERN",
      "value": "[-+][a-zA-Z0-9]"
    },
    "long_option": {
      "type": "PATTERN",
      "value": "--[a-zA-Z][a-zA-Z0-9\\-]*"
    },
    "option_section": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "option_line"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "option_description"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "option": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "[+-][a-zA-Z0-9-]+"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "PATTERN",
              "value": ", -[a-zA-Z0-9-]+"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "option_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": " {7}"
        },
        {
          "type": "SYMBOL",
          "name": "option_list"
        },
        {
          "type": "PATTERN",
          "value": "\\r?\\n"
        }
      ]
    },
    "option_list": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "short_option"
            },
            {
              "type": "SYMBOL",
              "name": "short_option_value"
            },
            {
              "type": "SYMBOL",
              "name": "short_option_value_pair"
            },
            {
              "type": "SYMBOL",
              "name": "long_option"
            },
            {
              "type": "SYMBOL",
              "name": "value_option_bracket"
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": ","
              },
              {
                "type": "PATTERN",
                "value": "[ \\t]+"
              },
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "SYMBOL",
                    "name": "short_option"
                  },
                  {
                    "type": "SYMBOL",
                    "name": "short_option_value"
                  },
                  {
                    "type": "SYMBOL",
                    "name": "short_option_value_pair"
                  },
                  {
                    "type": "SYMBOL",
                    "name": "long_option"
                  },
                  {
                    "type": "SYMBOL",
                    "name": "value_option_bracket"
                  }
                ]
              }
            ]
          }
        }
      ]
    },
    "short_option_value": {
      "type": "PATTERN",
      "value": "[-+][a-zA-Z0-9] <?[a-zA-Z0-9]+>?"
    },
    "short_option_value_pair": {
      "type": "PATTERN",
      "value": "[-+][a-zA-Z0-9] <[a-zA-Z0-9]+>=<[a-zA-Z0-9]+>"
    },
    "value_option_bracket": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "--[a-zA-Z][a-zA-Z0-9-]*="
        },
        {
          "type": "PATTERN",
          "value": "[a-zA-Z0-9-]+"
        }
      ]
    },
    "_bracket": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "STRING",
                "value": "<"
              },
              {
                "type": "STRING",
                "value": "("
              },
              {
                "type": "STRING",
                "value": "["
              }
            ]
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "REPEAT",
                "content": {
                  "type": "SYMBOL",
                  "name": "_bracket"
                }
              },
              {
                "type": "PREC",
                "value": -2,
                "content": {
                  "type": "PATTERN",
                  "value": "\\S+"
                }
              }
            ]
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "STRING",
                "value": ">"
              },
              {
                "type": "STRING",
                "value": ")"
              },
              {
                "type": "STRING",
                "value": "]"
              }
            ]
          }
        ]
      }
    },
    "option_description": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "REPEAT1",
        "content": {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_line_10"
            },
            {
              "type": "PATTERN",
              "value": "\\r?\\n"
            }
          ]
        }
      }
    }
  },
  "extras": [],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
