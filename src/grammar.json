{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "man",
  "rules": {
    "manual": {
      "type": "SEQ",
      "members": [
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "\\S[^\\r\\n]*\\r?\\n"
          },
          "named": true,
          "value": "title"
        },
        {
          "type": "PATTERN",
          "value": "\\r?\\n"
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "SYMBOL",
            "name": "section"
          }
        },
        {
          "type": "SYMBOL",
          "name": "footer"
        }
      ]
    },
    "section": {
      "type": "SEQ",
      "members": [
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "\\S[^\\r\\n]*\\r?\\n"
          },
          "named": true,
          "value": "section_title"
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "body"
              },
              {
                "type": "SYMBOL",
                "name": "subsection_heading"
              }
            ]
          }
        }
      ]
    },
    "subsection_heading": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "PATTERN",
            "value": " {3}"
          },
          {
            "type": "ALIAS",
            "content": {
              "type": "PATTERN",
              "value": "\\S[^\\r\\n]*\\r?\\n"
            },
            "named": true,
            "value": "subsection_title"
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "SYMBOL",
              "name": "body"
            }
          }
        ]
      }
    },
    "body": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "REPEAT1",
        "content": {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "option_section"
            },
            {
              "type": "SYMBOL",
              "name": "_block"
            }
          ]
        }
      }
    },
    "_block": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "REPEAT1",
            "content": {
              "type": "SYMBOL",
              "name": "_line"
            }
          },
          {
            "type": "PATTERN",
            "value": "\\r?\\n"
          }
        ]
      }
    },
    "_text": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "reference"
            },
            {
              "type": "TOKEN",
              "content": {
                "type": "PREC",
                "value": -2,
                "content": {
                  "type": "PATTERN",
                  "value": "[^\\r\\n]\\S*"
                }
              }
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "TOKEN",
                "content": {
                  "type": "PREC",
                  "value": -1,
                  "content": {
                    "type": "PATTERN",
                    "value": "\\S+"
                  }
                }
              },
              {
                "type": "SYMBOL",
                "name": "reference"
              },
              {
                "type": "PATTERN",
                "value": " +"
              }
            ]
          }
        }
      ]
    },
    "_line_10": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": " {10}"
        },
        {
          "type": "SYMBOL",
          "name": "_text"
        },
        {
          "type": "PATTERN",
          "value": "\\r?\\n"
        }
      ]
    },
    "_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": " {7}"
        },
        {
          "type": "SYMBOL",
          "name": "_text"
        },
        {
          "type": "PATTERN",
          "value": "\\r?\\n"
        }
      ]
    },
    "reference": {
      "type": "PATTERN",
      "value": "[a-zA-Z0-9_][a-zA-Z0-9_\\-]+\\(\\d+\\)"
    },
    "short_option": {
      "type": "PATTERN",
      "value": "\\-[a-zA-Z0-9]{1}"
    },
    "long_option": {
      "type": "PATTERN",
      "value": "--[a-zA-Z0-9][a-zA-Z0-9_\\-]+"
    },
    "option_section": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "option_line"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "option_description"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "option": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "[+-][a-zA-Z0-9-]+"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "PATTERN",
              "value": ", -[a-zA-Z0-9-]+"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "option_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": " {7}"
        },
        {
          "type": "SYMBOL",
          "name": "option"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "PATTERN",
              "value": "[^\\r\\n]+"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "PATTERN",
          "value": "\\r?\\n"
        }
      ]
    },
    "option_description": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "REPEAT1",
        "content": {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_line_10"
            },
            {
              "type": "PATTERN",
              "value": "\\r?\\n"
            }
          ]
        }
      }
    }
  },
  "extras": [],
  "conflicts": [],
  "precedences": [],
  "externals": [
    {
      "type": "SYMBOL",
      "name": "footer"
    }
  ],
  "inline": [],
  "supertypes": []
}
